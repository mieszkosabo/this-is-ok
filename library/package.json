{
  "name": "this-is-ok",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup --entry src/index.ts --entry src/option/option.ts --format esm,cjs --dts --minify",
    "dev": "tsup --entry src/index.ts --entry src/option/option.ts --format esm,cjs --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "bun": "^0.6.6",
    "bun-types": "^0.6.6",
    "eslint": "^7.32.0",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vitest": "^0.31.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
